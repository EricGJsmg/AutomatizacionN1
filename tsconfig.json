{
  "compilerOptions": {
    "target": "es2020",
    "lib": [
      "es2020",
      "dom"
    ],
    "types": [
      "node",
      "cypress"
    ],
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true
  },
  "include": [
    "cypress/**/*.ts",
    "cypress/**/*.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
